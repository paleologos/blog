<% include ./partials/header %>
<div class="row">
  <div class="col-lg-12">
    <h1 class="text-info">Home Page</h1>

  </div>
  <div class="col-lg-4">
    <% foundPosts.forEach(function(found){ %>
      <div class="">
        <h3 class="text-info"><%= found.title %></h3>
        <h5 class="text-info">Category: <%= found.category %></h5>
        <img src="<%= found.image %>" alt="">
        <p class="text-info"><%= found.content %></p>
        <a class="btn btn-success" href=" /posts/<%= found._id %>">Read more</a>

      </div>
    <% }); %>

  </div>

</div>
<% include ./partials/footer %>
